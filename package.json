{
  "name": "xc-game",
  "version": "0.0.0",
  "scripts": {
    "build": "npm-run-all --parallel build:*",
    "prebuild": "mkdir -p build/static",
    "watch:js": "DEV=true watchify -v --debug src/main.js -o build/static/bundle.js -t envify -t babelify",
    "build:js": "browserify  --debug src/main.js -o build/static/bundle.js -t envify -t babelify",
    "postbuild:js": "uglifyjs build/static/bundle.js -o build/static/bundle.js",
    "serve": "http-server -c-1",
    "serve:docs": "documentation serve src/main.js",
    "dev": "npm-run-all --parallel watch:js serve"
  },
  "dependencies": {
    "axios": "^0.15.3",
    "babel-plugin-transform-flow-strip-types": "^6.21.0",
    "babel-preset-es2015": "^6.18.0",
    "babelify": "^7.3.0",
    "box-intersect": "^1.0.1",
    "browserify": "^13.3.0",
    "envify": "^4.0.0",
    "lodash": "^4.17.4",
    "mustache": "^2.3.0",
    "npm-run-all": "^4.0.1",
    "snapsvg": "^0.4.0",
    "three": "^0.84.0",
    "uglifyjs": "^2.4.10",
    "watchify": "^3.8.0"
  },
  "devDependencies": {
    "http-server": "*"
  },
  "private": true,
  "license": "GPLV2"
}
